{
  "version": "2.0",
  "global": {
    "max_volume": 0.5
  },
  "amplifiers": {
    "amp1": {
      "card": "amp1",
      "channels": 8
    },
    "amp2": {
      "card": "amp2",
      "channels": 8
    },
    "amp3": {
      "card": "amp3",
      "channels": 8
    }
  },
  "speakers": {
    "wohnzimmer_right": {
      "amplifier": "amp1",
      "channel": 3,
      "volume": 100,
      "latency": 0
    },
    "wohnzimmer_left": {
      "amplifier": "amp1",
      "channel": 4,
      "volume": 100,
      "latency": 0
    },
    "backupkueche_right": {
      "amplifier": "amp3",
      "channel": 5,
      "volume": 100,
      "latency": 0
    },
    "backupkueche_left": {
      "amplifier": "amp3",
      "channel": 6,
      "volume": 100,
      "latency": 0
    },
    "kueche_left": {
      "amplifier": "amp1",
      "channel": 5,
      "volume": 100,
      "latency": 0
    },
    "esszimmer_left": {
      "amplifier": "amp3",
      "channel": 3,
      "volume": 100,
      "latency": 0
    },
    "esszimmer_right": {
      "amplifier": "amp1",
      "channel": 6,
      "volume": 100,
      "latency": 0
    },
    "kueche_right": {
      "amplifier": "amp3",
      "channel": 4,
      "volume": 100,
      "latency": 0
    },
    "vordach_left": {
      "amplifier": "amp1",
      "channel": 1,
      "volume": 100,
      "latency": 0
    }
  },
  "rooms": {
    "wohnzimmer": {
      "name": "Wohnzimmer",
      "left": "wohnzimmer_left",
      "right": "wohnzimmer_right",
      "zones": []
    },
    "backupkueche": {
      "name": "Backupkueche",
      "left": "backupkueche_left",
      "right": "backupkueche_right",
      "zones": ["essen"]
    },
    "kueche": {
      "name": "Kueche",
      "left": "kueche_left",
      "right": "kueche_right",
      "zones": ["essen"]
    },
    "esszimmer": {
      "name": "Esszimmer",
      "left": "esszimmer_left",
      "right": "esszimmer_right",
      "zones": ["essen"]
    },
    "vordach": {
      "name": "Vordach",
      "left": "vordach_left",
      "right": null,
      "zones": []
    }
  },
  "zones": {
    "alle": {
      "name": "Uberall",
      "include_all": true
    },
    "essen": {
      "name": "Essen"
    }
  },
  "snapcast": {
    "server": "localhost",
    "streams": {
      "default": {
        "type": "pipe",
        "path": "/tmp/snapfifo",
        "sampleformat": "48000:16:2",
        "codec": "flac"
      },
      "spotify_kueche": {
        "type": "librespot",
        "name": "Essen"
      },
      "spotify_wohnen": {
        "type": "librespot",
        "name": "Wohnen"
      },
      "airplay_kueche": {
        "type": "airplay",
        "name": "Essen",
        "port": 7000
      },
      "airplay_wohnen": {
        "type": "airplay",
        "name": "Wohnen",
        "port": 7001
      }
    },
    "stream_targets": {
      "default": {
        "zones": [
          "alle"
        ]
      },
      "spotify_wohnen": {
        "rooms": [
          "wohnzimmer"
        ]
      },
      "spotify_kueche": {
        "zones": [
          "essen"
        ]
      }
    }
  }
}