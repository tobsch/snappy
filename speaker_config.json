      "latency": 0
    },
    "elternschlafzimmer_right": {
      "amplifier": "amp2",
      "channel": 7,
      "volume": 100,
      "latency": 0
    },
    "gaestebad_right": {
      "amplifier": "amp3",
      "channel": 1,
      "volume": 100,
      "latency": 0
    },
    "esszimmer_right": {
      "amplifier": "amp3",
      "channel": 2,
