    "backupkueche_right": {
      "amplifier": "amp3",
      "channel": 4,
      "volume": 100,
      "latency": 0
    },
    "kinderzimmer_linda_right": {
      "amplifier": "amp3",
      "channel": 5,
      "volume": 100,
      "latency": 0
    },
    "kinderzimmer_linda_left": {
      "amplifier": "amp3",
      "channel": 6,
      "volume": 100,
      "latency": 0
    },
    "wohnzimmer_left": {
      "amplifier": "amp3",
      "channel": 7,
      "volume": 100,
      "latency": 0
    },
    "elternschlafzimmer_left": {
      "amplifier": "amp3",
      "channel": 8,
      "volume": 100,
      "latency": 0
    },
    "vordach_left": {
      "amplifier": "amp1",
      "channel": 1,
      "volume": 100,
      "latency": 0
    },
    "vordach_right": {
      "amplifier": "amp1",
      "channel": 2,
      "volume": 100,
